{
  "name": "ngxmatfire",
  "version": "1.5.1",
  "author": "Jeroen Ouwehand",
  "description": "Full stack starter app with Angular 8, Material Design and Firebase.",
  "keywords": [
    "angular",
    "angular 8",
    "firebase",
    "material",
    "nosql",
    "oauth",
    "starter-template",
    "ngx"
  ],
  "repository": "https://github.com/jeroenouw/AngularMaterialFirebase",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "reset": "rm -rf ./node_modules ./package-lock.json && npm install",
    "dist": "ng build",
    "prod": "ng build --prod",
    "prod:hashless": "ng build --prod --output-hashing none",
    "prod:src": "ng build --prod --sourcemaps",
    "compodoc:g": "compodoc -p src/tsconfig.app.json",
    "compodoc:s": "compodoc -p src/tsconfig.app.json --serve",
    "compodoc:w": "compodoc -p src/tsconfig.app.json --serve --watch",
    "tscov": "tscov -m 90",
    "tscov:d": "tscov --details",
    "link-upstream": "git remote add upstream https://github.com/jeroenouw/AngularMaterialFirebase",
    "sync": "git fetch && git checkout master && git merge upstream/master && git push"
  },
  "dependencies": {
    "@angular-material-extensions/password-strength": "^6.0.0",
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "^9.0.7",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/fire": "^5.4.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.7",
    "@angular/google-maps": "^9.1.3",
    "@angular/material": "^9.0.7",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/platform-server": "^9.0.7",
    "@angular/router": "^9.0.7",
    "core-js": "^3.6.4",
    "firebase": "^7.12.0",
    "hammerjs": "^2.0.8",
    "ngx-auth-firebaseui": "^4.0.1",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "ts-helpers": "^1.1.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.7",
    "@compodoc/compodoc": "^1.1.11",
    "@liftr/tscov": "^1.4.5",
    "@types/jasmine": "^3.5.9",
    "@types/node": "^13.9.2",
    "codelyzer": "^5.2.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.2",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "^8.7.0",
    "tslint": "^6.1.0",
    "typescript": "3.7.5",
    "webpack": "^4.42.0"
  },
  "peerDependencies": {
    "typescript": "3.8.3"
  }
}
